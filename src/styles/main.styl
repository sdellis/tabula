@import 'yeticss'

header
  padding-top: 50px

.label
  height: 40px

  .label-color
    width: 24px
    height: 24px
    border: 1px solid grey
    border-radius: 20px
    display: inline-block

  > span
    margin-left: 10px
    margin-right: 10px

.sequence
  padding: 0;
  margin: 0;

  display: flex;

  -webkit-flex-flow: row wrap;
  justify-content: space-around;

.slide
  background: tomato;
  padding: 10px 10px 0px 10px;
  width: 200px;
  margin-top: 10px;
  list-style: none;
  color: white;
  font-weight: bold;
  font-size: 3em;
  text-align: center;
  border-radius: 5px;
